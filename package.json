{
  "name": "versioned-ui",
  "version": "0.1.0",
  "private": true,
  "engines" : {
    "node": "8.11.2"
  },
  "scripts": {
    "serve": "bin/write-env && $(yarn bin)/vue-cli-service serve --open",
    "build": "bin/write-env && $(yarn bin)/vue-cli-service build",
    "lint": "$(yarn bin)/vue-cli-service lint",
    "dev": "VUE_APP_API_URL=http://localhost:3000/v1 yarn serve",
    "start": "node server.js",
    "heroku-postbuild": "yarn install --production=false && yarn build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e"
  },
  "dependencies": {
    "@vue/eslint-config-standard": "3.0.0-beta.9",
    "algoliasearch": "3.24.11",
    "axios": "0.17.1",
    "express": "4.16.2",
    "vue": "2.5.16",
    "vue-router": "3.0.1",
    "vuex": "3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.0.0-beta.1",
    "@vue/cli-plugin-e2e-cypress": "^3.0.0-beta.11",
    "@vue/cli-plugin-eslint": "3.0.0-beta.1",
    "@vue/cli-plugin-unit-jest": "3.0.0-beta.11",
    "@vue/cli-service": "3.0.0-beta.1",
    "@vue/test-utils": "1.0.0-beta.16",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "22.4.3",
    "cheerio": "1.0.0-rc.2",
    "vue-template-compiler": "2.5.16"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "standard"
    ],
    "rules": {
      "no-throw-literal": 0,
      "vue/require-v-for-key": false
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
